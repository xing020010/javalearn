<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="album.Album" table="album" catalog="acc">
		<id name="id" type="java.lang.Integer">
			<column name="id" />
			<generator class="native" />
		</id>
		<property name="uid" type="java.lang.Integer">
			<column name="uid" length="10" />
		</property>
		<property name="type" type="java.lang.String">
			<column name="type" length="20" />
		</property>

		<set name="images" table="image" lazy="true">
			<key>
				<column name="aid" not-null="true"></column>
			</key>
			<element type="string" column="name" not-null="true"></element>
		</set>
	</class>
</hibernate-mapping>